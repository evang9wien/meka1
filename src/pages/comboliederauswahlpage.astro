---
import Layout from '~/layouts/PageLayout.astro';
import Comboliederauswahl from '~/components/svelte/Comboliederauswahl.svelte';

const metadata = {
  title: 'Contact',
};
---

<head>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/svelte-material-ui@7.0.0-beta.15/bare.min.css" />
  <link rel="stylesheet" href="https://unpkg.com/@material/typography@13.0.0/dist/mdc.typography.css" />
  <!-- Hint where we get fonts from. -->
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />

  <!-- Material Icons, Roboto, and Roboto Mono fonts -->
  <link
    href="https://fonts.googleapis.com/css2?family=Material+Icons&Roboto+Mono:ital@0;1&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
    rel="stylesheet"
  />
  <!-- SMUI Styles -->
  <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/svelte-material-ui@7.0.0-beta.15/themes/svelte.min.css"
    media="(prefers-color-scheme: light)"
  />
  <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/svelte-material-ui@7.0.0-beta.15/themes/svelte-dark.min.css"
    media="screen and (prefers-color-scheme: dark)"
  />
</head>

<Layout metadata={metadata}>
  <Comboliederauswahl client:load />
</Layout>
